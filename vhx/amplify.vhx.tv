    // PUBLIC METHODS
    getRandomBG: function(stylesToSkip) {
  
      if (VHXJS.bgColors.length >= 0){
  
        var bgColorList = Array();
        // should we remove some styles for this picking?
        if (!stylesToSkip){
          bgColorList = VHXJS.bgColors;
        } else {
          var excludedStyles = stylesToSkip.split(",");
          $(VHXJS.bgColors).each(function(ind, eVal){
            // remember, we're excluding styles here. so if we get a -1 response, that's GOOD
            if ($.inArray(eVal, excludedStyles) < 0){
              bgColorList.push(eVal);
            }
          });
        }
        // pick a random value from the array and spit it back
        var randomBG = Math.floor(Math.random() * bgColorList.length);
        return bgColorList[randomBG];
      }
    },
