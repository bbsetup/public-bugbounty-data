    __positionFooter: function() {
        var siteFooter = $("footer.footer");
        var siteHeader = $("header.header");
        var siteFeature = $(".feature");
        var mainContent = $(".main");
        var windowHeight = $(window).height();
  
        
        if ((($(document.body).height() + siteFooter.outerHeight()) < $(window).height() && siteFooter.css("position") == "fixed") || ($(document.body).height() < $(window).height() && siteFooter.css("position") != "fixed")) {
            siteFooter.css({
                position: "fixed",
                bottom: "0px",
                width: "100%"
            });
        } else {
            siteFooter.css({
                position: "static"
            });
        }
        if (($(document.body).height() - $(".footer").outerHeight()) < $(window).height()){
         var footerPosition = siteFooter.position().top;
         var mainContentPosition = mainContent.position().top;
         var contentHeight = 0;
         
         if (windowHeight < VHXJS.windowHeightStorage){
         		contentHeight =  mainContent.height() - (VHXJS.windowHeightStorage - windowHeight);
         } else {
  	       contentHeight = footerPosition - mainContentPosition;
         }
         if (contentHeight >= VHXJS.contentHeightStorage){
  	       mainContent.css({height: contentHeight});
         } else {
         	mainContent.css({height: 'auto'});
         }
         VHXJS.windowHeightStorage = windowHeight;
        }
    },
