  h2, h3, h4, h5, h6 { font-family: 'Enriqueta', sans-serif; }
  /* START VHX THEME */
  /*
   __  __  __  __   __   __     
  /\ \/\ \/\ \/\ \ /\ \ /\ \    
  \ \ \ \ \ \ \_\ \\ `\`\/'/'   
   \ \ \ \ \ \  _  \`\/ > <     
    \ \ \_/ \ \ \ \ \  \/'/\`\  
     \ `\___/\ \_\ \_\ /\_\\ \_\
      `\/__/  \/_/\/_/ \/_/ \/_/
  */
  body {
  	background-color: #ecebeb;
    font-family: 'Enriqueta',sans-serif;
    -webkit-font-smoothing: antialiased;
    font-smoothing: antialiased;
    min-width: 320px;
  }
  body.open {
  	position: fixed;
  }
  ul { margin: 0; padding: 0; list-style: none; }
  .grey { color: #b8b8b8; }
  .bg_white { background-color: #FFF; }
  .bg_one, .bg_default { background-color: #f8bb4e; }
  .bg_two { background-color: #67c3ab; }
  .bg_three { background-color: #f4864f; }
  .bg_four { background-color: #4f99f4; }
  .bg_party { background-image: url('https://dr56wvhu2c8zo.cloudfront.net/yewknee/assets/e82cb82b-8c30-40a4-ad63-bdd1b41e4fc7/geometry-transparent.png'); };
  
  .border-top { border-top: 1px solid #ecebeb; }
  .border-bottom {   border-bottom: 1px solid #ecebeb; }
  .border-left { border-left: 1px solid #ecebeb; }
  .border-right { border-right: 1px solid #ecebeb; }
  .border { border: 1px solid #ecebeb; }
  /* --------------------------------------------------------------------------------------- */
  /* HEADER */
  /* --------------------------------------------------------------------------------------- */
  .header {
  	background-color: #FFF;
    padding: 50px 0;
  }
  .header h1 {
    font-weight: 900;
    margin: 0;
    line-height: 1;
  }
  .header .desktop-header h1 {
  	text-align: center;
  }
  .header .desktop-header .nav {
  	text-align: center;
  }
  .header.has-children .desktop-header .nav {
  	text-align: left;
  }
  .header a {
  	color: #000;
  }
  .header .nav {
  	padding-left: 4px;
    padding-top: 10px;
  }
  .header .nav a {
  	margin-right: 10px;
  }
  .header .mobile-header .small-1 a {
  	display: block;
    width: 50px;
    height: 50px;
  }
  .header.has-children .mobile-header small-1 {
  	display: none;
  }
  
  .buy-block {
  	position: relative;
  }
  .buy-block a {
  	display: block;
    background: #000;
    color: #FFF;
    font-size: 20px;
    padding: 0;
    text-align: left;
     font-family: 'Enriqueta',sans-serif;
     line-height: 1;
     height: 74px;
     -webkit-transition: all 0s ease;
    -moz-transition: all 0s ease;
    -ms-transition: all 0s ease;
    -o-transition: all 0s ease;
    transition: all 0s ease;
  }
  .buy-block a span.price {
  	font-size: 20px;
    display: block;
    background: #201e1c;
    border-right: 1px solid #373431;
    padding: 25px;
    float: left;
    height: 74px;
    min-width: 85px;
    text-align: center;
  }
  .buy-block a span.txt {
  	text-align: center;
    display: block;
    padding: 27px 0;
