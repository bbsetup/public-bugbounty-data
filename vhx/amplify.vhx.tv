    // utility function
    __componentToHex: function(c) {
      var hex = c.toString(16);
      return hex.length == 1 ? "0" + hex : hex;
    },
    // utility function
    __rgbToHex: function(r, g, b) {
      return "#" + VHXJS.__componentToHex(r) + VHXJS.__componentToHex(g) + VHXJS.__componentToHex(b);
    },
    _setupGradients: function(){
      if ($(".fade-out").length > 0){
        // add the gradient div
        $(".fade-out").each(function(){
          $(this).append("<div class='gradient'></div>");
          // get the bg color value from .content
          var colour = $(this).css("background-color");
          colour = colour.replace("rgb(","");
          colour = colour.replace(")","");
          colour = colour.replace(/ /g,"");
          colourArr = colour.split(",");
          // converts the bg color from browser rgb to usable hex
          var bgHex = VHXJS.__rgbToHex(parseInt(colourArr[0]),parseInt(colourArr[1]),parseInt(colourArr[2]));
  
          // $('.test').css({ 'background': '-webkit-gradient(linear,left top,left bottom,from(#f4f4f4),to(#FFFFFF))' });
          // build the before gradient
          var prefixes = new Array("-moz-linear-","-webkit-linear-","-o-linear-","-ms-linear-","linear-");
          var beforestr, afterstr = "";
          var beforedir = "top";
          var gradientstr = "";
  
          for(var i =0;i<=prefixes.length-1;i++){
            if (i == prefixes.length-1){
              beforedir = "to bottom";
            }
            // before = bottom
            gradientstr = prefixes[i]+"gradient("+beforedir+",  rgba("+colourArr[0]+","+colourArr[1]+","+colourArr[2]+",0) 0%, rgba("+colourArr[0]+","+colourArr[1]+","+colourArr[2]+",1) 100%)";
          } // end for
  
          $(this).find(".gradient").css("background",gradientstr);
        }); // end each
      } // if
    },
  
    _theaters: {
        init: function(theList) {
          var jqxhr = $.get(theList, function(data) {
            VHXJS._theaters.process(data);
          });
        },
        process: function(data){
          // first, split on the line break
          dataChunk = data.split("\n");
          // greater than 1 because the first row is labels
          if (dataChunk.length > 1){
            // next, swoop through and split on the comma
            vendors = new Array();
            for (var i=1;i<=dataChunk.length-1; i++){
              vendors.push(dataChunk[i].split(","));
            }
            VHXJS._theaters.display(vendors);
          } else {
            // no results, show error
            VHXJS._theaters.noShows();
          }
        },
        display: function(data){
  
          var eventTotal = data.length - 1;
          var eventHalf = Math.floor(eventTotal/2);
          var eventCounter = 0;
  
          var thehtml = "";
  
          for (var i=0;i<=data.length-1; i++){
            // 0 = Jan 30th
            // 1 = 2013
            // 2 = Miami
            // 3 = Fl
            // 4 = O-cinema
            // 5 = http://ticketlink.org
            // 6 = addtl
            // 7 = Is Past? flag
            var ispast = data[i][7];
            var rowclass = "";
            var thedate = data[i][0] + ", " + data[i][1];
            var cityst = data[i][2] + ", " + data[i][3];
            var venue = data[i][4].replace("\"","");
            thedate = thedate.replace(/"/g,"");
            cityst = cityst.replace(/"/g,"");
  
            thehtml += '<div class="row">';
            thehtml += '  <div class="small-12 medium-12 large-3 columns date"><span>'+thedate+'</span></div>';
            thehtml += '  <div class="small-12 medium-6 large-4 columns cityst"><span>'+cityst+'</span></div>';
            thehtml += '  <div class="small-12 medium-6 large-5 columns cinema"><span>'+venue+'</span></div>';
            thehtml += '</div>';
  
            eventCounter++;
  
          }
          //thehtml += "</div>";
         $("#nowplaying").html(thehtml);
        },
        noShows: function(){
          $("#nowplaying").html('<div class="row"><div class="small-12">Sorry, no screenings at this time.</div></div>');
        }
     }
