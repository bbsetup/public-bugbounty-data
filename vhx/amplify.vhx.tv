      if ($(".synopsis").length > 0){
        var numparagraphs = $(".synopsis .txt p").length;
        switch (numparagraphs)
        {
          case 1:
             if ($(".synopsis .txt p").html() == ""){
              $(".synopsis").remove();
              $("a[href^='/#about']").remove();
             } else {
              $(".synopsis .txt").addClass("larger");
             }
            break;
          case 2:
            $(".synopsis .txt").addClass("two-col");
            break;
  
          case 3:
            $(".synopsis .txt").addClass("three-col");
            break;
  
          default:
            break;
         }
      }
      // destroy the synopsis altogether if its blank
      if ($(".synopsis .txt p").html() == ""){
        $(".synopsis").hide();
      }
