            <div class="image-caption fg-white" data-ng-show="photo.imageCaption">
                <span class="{{photo.captionPlacement}}">{{photo.imageCaption}}</span>
            </div>

            <picture>
                <source data-ng-repeat="breakpoint in newBreakpoints track by $index"
                        srcset="{{breakpoint.imagePath}}"
                        media="(min-width: {{breakpoint.imageBreakpoint}}px)"/>
                <img data-ng-src="{{newBreakpoints[0].imagePath}}" alt="{{photo.imageAltTxt}}"/>
