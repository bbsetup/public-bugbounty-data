            // facebook
            function loginFacebookUser() {
                FB.login(function (response)
                    {
                        //handle the response
                    },
                    { scope: 'public_profile,email,user_friends', return_scopes: true, auth_type: 'rerequest'}
                );
            }

            window.fbAsyncInit = function() {
                FB.init({
                    appId: '189309987769660',
                    cookie: true,  // enable cookies to allow the server to access the session
                    xfbml: true,  // parse social plugins on this page
                    version: 'v2.9', // use version 2.9
                    channelUrl: '//19719.m.viator.com/fb_channel.php'
                });

                //FB.Event.subscribe('auth.statusChange', statusChangeCallback);
                FB.Event.subscribe('auth.authResponseChange', statusChangeCallback);
            };

            // Load the SDK asynchronously
            (function(d, s, id) {
                var js, fjs = d.getElementsByTagName(s)[0];
                if (d.getElementById(id)) return;
                js = d.createElement(s); js.id = id;
                js.src = "//connect.facebook.net/en_US/sdk.js";
                fjs.parentNode.insertBefore(js, fjs);
            }(document, 'script', 'facebook-jssdk'));

            // If someone logs in with google or FB on any of these pages, reload the page
            // otherwise it reloads the header element only.
            function reloadPageOnLogin() {
                var current_file = window.location.pathname.split('/').pop();

                if ((current_file == 'join.php') ||
                (current_file == 'login.php') ||
                (current_file == 'logout.php') ||
                (current_file == 'accountSettings.php') ||
                (current_file == 'checkOutCart.php'))
                return true;

                return false;
            }

            // for facebook
            function statusChangeCallback(response) {
                if (response.status === 'connected')  // logged in
                {
                    $.get("/fb_status_change.php", {
                            loggedin: true,
                            accessToken: response.authResponse.accessToken,
                            userID: response.authResponse.userID
                        },
                        function (json) {
                            var data = JSON.parse(json);
                            if(data.success != false) {
                                if (data.reload_page == true) {
                                    location.reload(true); // refresh the page.
                                }
                            } else {
                                alert("Viator requires your email address to send important information about your bookings. Please log into Facebook and make sure you have included your email address.");
                            }
                        }
                    );
                }
                else // logged out.
                {
                    $.get("/fb_status_change.php", {
                            loggedin: false
                        },
                        function (json) {
                            var data = JSON.parse(json);
                            //myLog(json);
                            if (data.reload_page) {
                                location.reload(true); // refresh the page.
                            }
                            else {
                                $.get('/login_topbar.php', function (data) {
                                    // replace the div on the page.
                                    $('#topbar').replaceWith(data);
                                    hideLoadingSpinner();
                                });
                            }
                        }
                   );
                }
            }


