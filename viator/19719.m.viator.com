				                    // replace the div on the page.
				                    $('#topbar').replaceWith(data);
				                    hideLoadingSpinner();
				                });
				            }
				        }
				    });
	        }
        }


        // use a less common namespace than just 'log'
        function myLog(msg) {
            // attempt to send a message to the console
            try {
                console.log(msg);
            }
            // fail gracefully if it does not exist
            catch (e) { }
        }
