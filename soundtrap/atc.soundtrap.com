    <script type="text/ng-template" id="/public/dialogs/signup/personal/header.html">
      <h4 class="title">
  <span class="soundtrap-logo"><img src="/public/images/Soundtrap_Symbol_White.svg" alt="Soundtrap"></span>
  {{ dialog.title || ('view.dialogs.signup_beta.title' | translate) }}
</h4>    </script>

    <script type="text/ng-template" id="/public/dialogs/signup/personal/body.html">
      <div class="controller-wrapper" ng-controller="LoginCtrl as login"
     ng-init="login.setup({'edu': !!dialog.extraOptions.edu, 'eduTrialSignup': eduTrialSignup, 'url': signup.dialog.extraOptions.url, 'eduCode': dialog.extraOptions.eduCode})">
  <div class="columns" ng-controller="SignupDialogCtrl as signup" ng-class="{ 'email-shown': signup.isEmailFormShown }"
       ng-init="signup.eduTrialSignup = eduTrialSignup; signup.eduCode = dialog.extraOptions.eduCode; signup.edu = !!dialog.extraOptions.edu; signup.url = dialog.extraOptions.url || '';">
    <div class="buttons" ng-if="!login.needsAgeCheck">
      <div>
        <div class="login-button login-button-gp google-white"><a class="btn btn-xs-block google-signup-button" ng-click="login.loginGoogle()"><span
            class="signupdivider"></span>{{'view.dialogs.signup_beta.sign_up_with' | translate}} <strong>Google</strong></a></div>
        <div class="login-button login-button-fb" ng-if="!signup.isEduSignup()"><a class="btn btn-xs-block" ng-click="login.loginFacebook()"><span class="signupdivider"></span>{{'view.dialogs.signup_beta.sign_up_with'
          | translate}} <strong>Facebook</strong></a></div>
        <div class="login-button login-button-ms" ng-if="signup.isEduSignup()"><a class="btn btn-xs-block" ng-click="login.loginMicrosoft()"><span class="signupdivider"></span>{{'view.dialogs.signup_beta.sign_up_with'
          | translate}} <strong>Office 365</strong></a></div>
      </div>
      <a href="" class="login-link login-link-email" ng-click="signup.onShowEmail()"><span
          class="glyphicons glyphicons-envelope"></span>{{'view.dialogs.signup_beta.email_instead_link' | translate}}</a>
      <a href="" ng-click="signup.goToLogin()">{{'view.dialogs.signup_beta.already_has_account_personal' | translate}}</a>
    </div>
    <div class="email-form" ng-if="!login.needsAgeCheck">
      <form class="signup-form" ng-submit="signup.submit()" id="signup-form" name="signup.formSignup">
        <div ng-repeat="error in signup.errorMessage" class="alert alert-danger" ng-show="error" ng-bind-html="error"></div>

        <label for="l_email" translate="view.dialogs.signup_beta.email_signup_headline"></label>

        <div class="form-group">
          <input ng-class="{'st-error': signup.errorMessage.email}" placeholder="{{ 'view.dialogs.signup_beta.email_placeholder' | translate }}"
                 type="text" class="form-control" ng-model-options="{ updateOn: 'blur' }" id="l_email" name="email"
                 title="{{ 'view.dialogs.signup_beta.email_placeholder' | translate }}" ng-model="signup.email" tabindex="1" required/>
        </div>

        <div class="form-group">
          <input ng-class="{'st-error': signup.errorMessage.password}" placeholder="{{ 'view.dialogs.signup_beta.password_placeholder' | translate }}"
                 type="password" class="form-control" id="l_password" name="password"
                 title="{{ 'view.dialogs.signup_beta.password_placeholder' | translate }}" ng-model="signup.password"
                 ng-model-options="{ updateOn: 'blur' }" tabindex="2" required/>
        </div>

        <div ng-if="signup.isEduSignup() && !signup.hideNameField">
          <label for="l_firstName" translate="view.dialogs.signup_beta.name.label"></label>

          <div class="form-group">
            <input ng-class="{'st-error': signup.errorMessage.first_name}" type="text" class="form-control" id="l_firstName" name="firstName"
                   title="{{ 'view.dialogs.signup_beta.first_name.placeholder' | translate }}" ng-model="signup.firstName"
                   ng-model-options="{ updateOn: 'blur' }" tabindex="3" required placeholder="{{ 'view.dialogs.signup_beta.first_name.placeholder' | translate }}" />
          </div>

          <div class="form-group">
            <input ng-class="{'st-error': signup.errorMessage.last_name}" type="text" class="form-control" id="l_lastName" name="lastName"
                   title="{{ 'view.dialogs.signup_beta.last_name.placeholder' | translate }}" ng-model="signup.lastName"
                   ng-model-options="{ updateOn: 'blur' }" tabindex="3" required placeholder="{{ 'view.dialogs.signup_beta.last_name.placeholder' | translate }}" />
          </div>
        </div>

        <span ng-if="!signup.isEduSignup()">
          <label for="signup-country" translate="view.dialogs.signup.choose_country"></label>

          <div id="signup-country" class="form-group" ng-init="signup.input.country">
            <select required ng-options="country.code as country.name for country in signup.countries" ng-model="signup.countryCode"
                    class="form-control" name="country" tabindex="3">
              <option value="" disabled translate="view.dialogs.signup.country"></option>
            </select>
          </div>

          <label for="signup-date-of-birth" translate="view.dialogs.signup.date_of_birth"></label>

          <date-selector date="signup.birthDate" tab-index="4"></date-selector>
        </span>

        <div ng-if="signup.edu && !signup.eduTrialSignup">
          <label for="l_eduCode" translate="view.dialogs.signup_beta.edu_code.label"></label>
          <div class="form-group">
            <input ng-class="{'st-error': signup.errorMessage.eduCode}" type="text" class="form-control" id="l_eduCode" name="eduCode"
                   title="{{ 'view.dialogs.signup_beta.edu_code.title' | translate }}" ng-model="signup.eduCode"
                   ng-model-options="{ updateOn: 'blur' }" tabindex="3" required ng-disabled="signup.isEduCodeLocked" />
          </div>
        </div>

        <!-- grecaptcha-placeholder div must be empty, otherwise recaptcha library refuses to render anything in it -->
        <div id="grecaptcha-placeholder" style="display:none"></div>

        <div class="text-center">
          <input ng-if="signup.canGoBackToTrialDialog" type="button" value="{{ 'view.dialogs.signup_beta.back_btn' | translate }}"
                 class="btn btn-trans" ng-click="signup.backToTrialDialog()"/>
          <input type="submit" id="s_submit" value="{{ 'view.dialogs.signup_beta.sign_up_btn' | translate }}" class="btn button_D_pr_L"
                 tabindex="16"/>
        </div>

        <div class="already spacer10" ng-if="!signup.canGoBackToTrialDialog">
          <div class="text-center"><a href="" class="small-link" ng-click="signup.goToLogin()"
                                      translate="view.dialogs.signup_beta.existing_user_label"></a></div>
        </div>
      </form>
    </div>
    <div class="age-check-form" ng-if="login.needsAgeCheck">
      <form ng-submit="login.submitAgeCheck()" id="age-check-form">
        <p translate="view.dialogs.signup.age_check_description"></p>

        <label for="age-check-country" translate="view.dialogs.signup.choose_country"></label>
        <div id="age-check-country" class="form-group" ng-init="login.input.country">
          <select required ng-options="country.code as country.name for country in login.countries" ng-model="login.countryCode" class="form-control"
                  tabindex="1">
            <option value="" disabled translate="view.dialogs.signup.country"></option>
          </select>
        </div>

        <label for="age-check-date-of-birth" translate="view.dialogs.signup.date_of_birth"></label>

        <date-selector date="login.birthDate" tab-index="2"></date-selector>

        <div class="text-center">
          <input type="submit" value="{{ 'view.dialogs.ok' | translate }}" class="btn button_D_pr_L" tabindex="16"/>
        </div>
      </form>
    </div>
  </div>
</div>
    </script>

