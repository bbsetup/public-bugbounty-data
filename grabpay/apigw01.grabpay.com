        jQuery(".grabpay-tab .tab_content").hide();
        jQuery(".grabpay-tab .tab_content:first-child").show();

      /* if in tab mode */
        jQuery(".grabpay-tab ul.tabs li").click(function() {

            // get parent IDs by class;
            var parentId = jQuery(this).parents('.grab-tabs').attr('id');

            jQuery("#"+parentId+" .tab_content").hide();
            var activeTab = jQuery(this).attr("rel"); 
            jQuery("#"+parentId+" #"+activeTab).fadeIn();

            // trigger swiper again;
            var id = jQuery('#'+activeTab+' .swiper-container').attr('id');
            new Swiper('#'+id, {
                                  navigation: {
                                    nextEl: '#'+id+' .gp-next',
                                    prevEl: '#'+id+' .gp-prev',
                                  },
                                  loop: true,
                                });

            jQuery("#"+parentId+" ul.tabs li").removeClass("active");
            jQuery(this).addClass("active");

            jQuery("#"+parentId+" .tab_drawer_heading").removeClass("d_active");
            jQuery("#"+parentId+" .tab_drawer_heading[rel^='"+activeTab+"']").addClass("d_active");

            // for mobile;
            //window.scrollTo(500, 0);
            // var wd = screen.width;
            // if(wd < 480){

            // }
            // console.log(screen.width);
        
        });
      /* if in drawer mode */
        jQuery(" .tab_drawer_heading").click(function() {
          
            // get parent IDs by class;
            var parentId = $(this).parents('.grab-tabs').attr('id');

            jQuery("#"+parentId+" .tab_content").hide();
            jQuery("#"+parentId+" .tab_content").removeClass('active_content');
            var d_activeTab = jQuery(this).attr("rel"); 
            jQuery("#"+parentId+" #"+d_activeTab).fadeIn();
            jQuery("#"+parentId+" #"+d_activeTab).addClass('active_content');

            jQuery("#"+parentId+" .tab_drawer_heading").removeClass("d_active");
            jQuery(this).addClass("d_active");

            jQuery("#"+parentId+" ul.tabs li").removeClass("active");
            jQuery("#"+parentId+" ul.tabs li[rel^='"+d_activeTab+"']").addClass("active");
        });

