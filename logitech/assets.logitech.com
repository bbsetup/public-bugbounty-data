        // LOGIWEB-2918 - Fix for smoothscroll
        // !!! NOTE MUST BE CHECKED AND REMOVE AFTER NEXT IMMEDIATE LAUNCH!!!!!!!
        pangea.smoothScroll.onClick = function(event, speed, topPadding) {
            
            console.log('test', pangea)
            if(speed === null || speed === undefined) {
                speed = 600;
            }
            if(topPadding === null || topPadding === undefined) {
                topPadding = 25;
