            if (!id || id.indexOf('#') !== 0 || $target.data('addToCart')) {
                return;
            }
           event.preventDefault();
           if (id === '#' || id === '#top') {
             pangea.smoothScroll.scrollToPx(0, speed);
              } else {
              pangea.smoothScroll.scrollToElm($(id), speed, topPadding);
           }
            pangea.smoothScroll.updateURL(id);
