<script>
function setCookie(cname,cvalue,exdays) {
  var d = new Date();
  d.setTime(d.getTime() + (exdays*24*60*60*1000));
  var expires = "expires=" + d.toGMTString();
  document.cookie = cname + "=" + cvalue + ";" + expires + ";path=/";
}

function getCookie(cname) {
  var name = cname + "=";
  var decodedCookie = decodeURIComponent(document.cookie);
  var ca = decodedCookie.split(';');
	//var ca = document.cookie.split(';');
  for(var i = 0; i < ca.length; i++) {
    var c = ca[i];
    while (c.charAt(0) == ' ') {
      c = c.substring(1);
    }
    if (c.indexOf(name) == 0) {
      return c.substring(name.length, c.length);
    }
  }
  return "";
}

function checkCookie() {
  var user = getCookie("blprivacypolicyprompt");

  console.log("Cookie currently is: ", user);
  if (user == "" || user == null) {
		$('#privacy-notification-overlay').show();
		console.log("Visitor is new, cookie being set now...");
		setCookie("blprivacypolicyprompt", "notified", 730);
		var updatedUser = getCookie("blprivacypolicyprompt");
		console.log("User's updated cookie: ", updatedUser);
  } else {
     //if (user != "" && user != null) {
		$('#privacy-notification-overlay').hide();
    console.log("Returning visitor.  Visitor has cookie: ",user);
     //}
  }
}

$('#privacy-notification-overlay-close, #privacy-notification-overlay button').click(function() {
	console.log("closing overlay");
	$('#privacy-notification-overlay').hide();
});

checkCookie();
//document.cookie = "nwprivacypolicyprompt=; expires=Thu, 01 Jan 2019 00:00:00 UTC; path=/;";


</script>



